{
	"$schema": "vscode://schemas/color-theme",
	"name": "AbacatePay Soft Italic Theme",
	"type": "dark",
	"colors": {
		"editor.background": "#222526",
		"editor.foreground": "#ECF0F1",
		"activityBar.background": "#222526",
		"activityBar.foreground": "#C6EF9D",
		"activityBar.activeBorder": "#C6EF9D",
		"activityBarBadge.background": "#C6EF9D",
		"activityBarBadge.foreground": "#222526",
		"sideBar.background": "#292C2D",
		"sideBar.foreground": "#ECF0F1",
		"sideBar.border": "#2E3132",
		"sideBarTitle.foreground": "#C6EF9D",
		"editorGroupHeader.tabsBackground": "#222526",
		"editorGroupHeader.tabsBorder": "#2E3132",
		"tab.activeBackground": "#292C2D",
		"tab.activeForeground": "#C6EF9D",
		"tab.activeBorder": "#C6EF9D",
		"tab.inactiveBackground": "#222526",
		"tab.inactiveForeground": "#94A3B8",
		"tab.border": "#2E3132",
		"statusBar.background": "#222526",
		"statusBar.foreground": "#ECF0F1",
		"statusBar.border": "#2E3132",
		"statusBarItem.hoverBackground": "#2E3132",
		"list.activeSelectionBackground": "#35393B",
		"list.activeSelectionForeground": "#C6EF9D",
		"list.hoverBackground": "#35393B",
		"list.highlightForeground": "#C6EF9D",
		"editor.lineHighlightBackground": "#292C2D",
		"editor.lineHighlightBorder": "#2E3132",
		"editor.selectionBackground": "#35393B",
		"editorCursor.foreground": "#C6EF9D",
		"editorWhitespace.foreground": "#3D5F61",
		"editorIndentGuide.background1": "#3D5F61",
		"editorIndentGuide.activeBackground1": "#C6EF9D",
		"titleBar.activeBackground": "#222526",
		"titleBar.activeForeground": "#C6EF9D",
		"titleBar.border": "#2E3132",
		"terminal.background": "#222526",
		"terminal.foreground": "#ECF0F1",
		"terminal.ansiBlack": "#222526",
		"terminal.ansiBlue": "#7A916E",
		"terminal.ansiCyan": "#3D5F61",
		"terminal.ansiGreen": "#C6EF9D",
		"terminal.ansiMagenta": "#D4A373",
		"terminal.ansiRed": "#E67E22",
		"terminal.ansiWhite": "#ECF0F1",
		"terminal.ansiYellow": "#FAF4C8",
		"terminal.ansiBrightBlack": "#94A3B8",
		"terminal.ansiBrightBlue": "#7A916E",
		"terminal.ansiBrightCyan": "#3D5F61",
		"terminal.ansiBrightGreen": "#D9F2C7",
		"terminal.ansiBrightMagenta": "#D4A373",
		"terminal.ansiBrightRed": "#E67E22",
		"terminal.ansiBrightWhite": "#ECF0F1",
		"terminal.ansiBrightYellow": "#FAF4C8"
	},
	"tokenColors": [
		{
			"name": "Markdown Heading 1",
			"scope": [
				"entity.name.section.markdown heading.1.markdown",
				"heading.1.markdown entity.name.section.markdown",
				"heading.1.markdown",
				"entity.name.section.markdown"
			],
			"settings": {
				"foreground": "#C6EF9D",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Heading 2",
			"scope": [
				"punctuation.definition.heading.markdown heading.2.markdown",
				"heading.2.markdown punctuation.definition.heading.markdown",
				"heading.2.markdown"
			],
			"settings": {
				"foreground": "#D9F2C7",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Heading 3",
			"scope": [
				"punctuation.definition.heading.markdown heading.3.markdown",
				"heading.3.markdown punctuation.definition.heading.markdown",
				"heading.3.markdown"
			],
			"settings": {
				"foreground": "#7A916E",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Heading 4",
			"scope": ["heading.4.markdown"],
			"settings": {
				"foreground": "#7A916E",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Heading 5",
			"scope": ["heading.5.markdown"],
			"settings": {
				"foreground": "#FAF4C8",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Heading 6",
			"scope": ["heading.6.markdown"],
			"settings": {
				"foreground": "#94A3B8",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Strong/Bold",
			"scope": [
				"markup.strong",
				"markup.bold",
				"markup.strong.markdown",
				"markup.bold.markdown"
			],
			"settings": {
				"foreground": "#D4A373",
				"fontStyle": "bold"
			}
		},
		{
			"name": "JSON Key - Level 0",
			"scope": [
				"support.type.property-name.json",
				"source.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#FAF4C8"
			}
		},
		{
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.quoted.docstring"
			],
			"settings": {
				"foreground": "#94A3B8",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"variable",
				"variable.other",
				"support.variable",
				"identifier",
				"variable.other.property",
				"variable.other.object.property",
				"meta.object-literal.key",
				"entity.name.variable.field"
			],
			"settings": {
				"foreground": "#D9F2C7"
			}
		},
		{
			"scope": ["variable.parameter"],
			"settings": {
				"foreground": "#D9F2C7",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"constant.language",
				"constant.numeric",
				"constant.character"
			],
			"settings": {
				"foreground": "#C6EF9D"
			}
		},
		{
			"scope": [
				"variable.other.constant"
			],
			"settings": {
				"foreground": "#FAF4C8"
			}
		},
		{
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.logical",
				"keyword.declaration",
				"keyword.function",
				"punctuation.accessor"
			],
			"settings": {
				"foreground": "#7A916E",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"storage.type",
				"storage.modifier",
				"storage.type.function",
				"keyword.declaration",
				"keyword.declaration.function",
				"storage.type.class",
				"storage.type.struct",
				"storage.type.enum",
				"storage.type.interface",
				"entity.name.namespace",
				"support.other.namespace",
				"support.type.object.module",
				"variable.other.object.package",
				"variable.other.constant.package"
			],
			"settings": {
				"foreground": "#7A916E",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["string", "punctuation.definition.string", "string.template"],
			"settings": {
				"foreground": "#BFA58A"
			}
		},
		{
			"scope": [
				"entity.name.function",
				"support.function",
				"variable.function"
			],
			"settings": {
				"foreground": "#D4A373"
			}
		},
		{
			"scope": ["entity.name.type", "support.type", "support.class"],
			"settings": {
				"foreground": "#FAF4C8"
			}
		},
		{
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#FAF4C8"
			}
		},
		{
			"scope": [
				"punctuation",
				"meta.brace",
				"meta.brace.round",
				"meta.brace.square",
				"meta.brace.curly",
				"punctuation.definition.parameters",
				"punctuation.terminator",
				"punctuation.section",
				"punctuation.separator",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#ECF0F1"
			}
		},
		{
			"name": "HTML/XML Tags",
			"scope": [
				"entity.name.tag.html",
				"entity.name.tag.xml",
				"entity.name.tag.builtin",
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#C6EF9D",
				"fontStyle": "italic"
			}
		},
		{
			"name": "JSX/TSX Components",
			"scope": ["entity.name.tag", "support.class.component"],
			"settings": {
				"foreground": "#FAF4C8",
				"fontStyle": "italic"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"foreground": "#7A916E"
			}
		},
		{
			"name": "HTML Attribute Values",
			"scope": [
				"string.quoted.double.html",
				"string.quoted.single.html",
				"punctuation.definition.string.begin.html",
				"punctuation.definition.string.end.html"
			],
			"settings": {
				"foreground": "#BFA58A"
			}
		},
		{
			"name": "HTML Punctuation",
			"scope": [
				"punctuation.separator.key-value.html",
				"punctuation.definition.tag.html"
			],
			"settings": {
				"foreground": "#ECF0F1"
			}
		},
		{
			"name": "Markdown Paragraph Text",
			"scope": ["meta.paragraph.markdown", "text.html.markdown"],
			"settings": {
				"foreground": "#ECF0F1"
			}
		},
		{
			"name": "Markdown Links",
			"scope": [
				"markup.link",
				"markup.underline.link",
				"meta.link.inline.markdown",
				"meta.link.reference.markdown"
			],
			"settings": {
				"foreground": "#D9F2C7"
			}
		},
		{
			"name": "Markdown Link Titles",
			"scope": [
				"string.other.link.title.markdown",
				"string.other.link.description.markdown",
				"punctuation.definition.string.begin.markdown",
				"punctuation.definition.string.end.markdown"
			],
			"settings": {
				"foreground": "#FAF4C8"
			}
		},
		{
			"name": "Markdown Code",
			"scope": [
				"markup.inline.raw",
				"markup.fenced_code.block",
				"punctuation.definition.markdown"
			],
			"settings": {
				"foreground": "#FAF4C8"
			}
		},
		{
			"name": "Markdown Italic",
			"scope": ["markup.italic", "punctuation.definition.italic.markdown"],
			"settings": {
				"foreground": "#7A916E",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown Bold",
			"scope": ["markup.bold", "punctuation.definition.bold.markdown"],
			"settings": {
				"foreground": "#D9F2C7",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Lists",
			"scope": ["beginning.punctuation.definition.list.markdown"],
			"settings": {
				"foreground": "#C6EF9D"
			}
		}
	]
}