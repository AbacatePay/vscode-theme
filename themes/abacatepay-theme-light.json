{
	"$schema": "vscode://schemas/color-theme",
	"name": "AbacatePay Theme Light",
	"type": "light",
	"colors": {
		"editor.background": "#F1F1F0",
		"editor.foreground": "#3F4E4F",
		"activityBar.background": "#F1F1F0",
		"activityBar.foreground": "#4C7A2E",
		"activityBar.activeBorder": "#3F4E4F",
		"activityBarBadge.background": "#4C7A2E",
		"activityBarBadge.foreground": "#F1F1F0",
		"sideBar.background": "#F0F0EF",
		"sideBar.foreground": "#3F4E4F",
		"sideBar.border": "#00000000",
		"sideBarTitle.foreground": "#3F4E4F",
		"editorGroupHeader.tabsBackground": "#F0F0EF",
		"editorGroupHeader.tabsBorder": "#00000000",
		"tab.activeBackground": "#F1F1F0",
		"tab.activeForeground": "#3F4E4F",
		"tab.activeBorder": "#3F4E4F",
		"tab.inactiveBackground": "#F0F0EF",
		"tab.inactiveForeground": "#708090",
		"tab.border": "#00000000",
		"statusBar.background": "#F1F1F0",
		"statusBar.foreground": "#3F4E4F",
		"statusBar.border": "#00000000",
		"statusBarItem.hoverBackground": "#E5E5E1",
		"list.activeSelectionBackground": "#E5E5E1",
		"list.activeSelectionForeground": "#3F4E4F",
		"list.hoverBackground": "#E5E5E166",
		"list.highlightForeground": "#4C7A2E",
		"editor.lineHighlightBackground": "#E5E5E166",
		"editor.lineHighlightBorder": "#00000000",
		"editor.selectionBackground": "#E5E5E1",
		"editorCursor.foreground": "#3F4E4F",
		"editorWhitespace.foreground": "#E5E5E1",
		"editorIndentGuide.background1": "#E5E5E1",
		"editorIndentGuide.activeBackground1": "#3F4E4F",
		"titleBar.activeBackground": "#F1F1F0",
		"titleBar.activeForeground": "#3F4E4F",
		"titleBar.border": "#00000000",
		"menubar.selectionForeground": "#3F4E4F",
		"menubar.selectionBackground": "#E5E5E1",
		"menu.foreground": "#3F4E4F",
		"menu.background": "#F1F1F0",
		"menu.selectionForeground": "#3F4E4F",
		"menu.selectionBackground": "#E5E5E1",
		"terminal.background": "#F1F1F0",
		"terminal.foreground": "#3F4E4F",
		"terminal.ansiBlack": "#3F4E4F",
		"terminal.ansiBlue": "#2F4F4F",
		"terminal.ansiCyan": "#2F4F4F",
		"terminal.ansiGreen": "#4C7A2E",
		"terminal.ansiMagenta": "#A0522D",
		"terminal.ansiRed": "#B22222",
		"terminal.ansiWhite": "#F1F1F0",
		"terminal.ansiYellow": "#B58900",
		"terminal.ansiBrightBlack": "#708090",
		"terminal.ansiBrightBlue": "#2F4F4F",
		"terminal.ansiBrightCyan": "#2F4F4F",
		"terminal.ansiBrightGreen": "#6A994E",
		"terminal.ansiBrightMagenta": "#A0522D",
		"terminal.ansiBrightRed": "#B22222",
		"terminal.ansiBrightWhite": "#F1F1F0",
		"terminal.ansiBrightYellow": "#B8860B"
	},
	"tokenColors": [
		{
			"name": "Markdown Heading 1",
			"scope": [
				"entity.name.section.markdown heading.1.markdown",
				"heading.1.markdown entity.name.section.markdown",
				"heading.1.markdown",
				"entity.name.section.markdown"
			],
			"settings": {
				"foreground": "#4C7A2E",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Heading 2",
			"scope": [
				"punctuation.definition.heading.markdown heading.2.markdown",
				"heading.2.markdown punctuation.definition.heading.markdown",
				"heading.2.markdown"
			],
			"settings": {
				"foreground": "#6A994E",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Heading 3",
			"scope": [
				"punctuation.definition.heading.markdown heading.3.markdown",
				"heading.3.markdown punctuation.definition.heading.markdown",
				"heading.3.markdown"
			],
			"settings": {
				"foreground": "#556B2F",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Heading 4",
			"scope": ["heading.4.markdown"],
			"settings": {
				"foreground": "#556B2F",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Heading 5",
			"scope": ["heading.5.markdown"],
			"settings": {
				"foreground": "#B58900",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Heading 6",
			"scope": ["heading.6.markdown"],
			"settings": {
				"foreground": "#708090",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Strong/Bold",
			"scope": [
				"markup.strong",
				"markup.bold",
				"markup.strong.markdown",
				"markup.bold.markdown"
			],
			"settings": {
				"foreground": "#A0522D",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.quoted.docstring"
			],
			"settings": {
				"foreground": "#708090",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"variable",
				"variable.other",
				"support.variable",
				"identifier",
				"variable.parameter",
				"variable.other.property",
				"variable.other.object.property",
				"meta.object-literal.key",
				"entity.name.variable.field"
			],
			"settings": {
				"foreground": "#3F4E4F"
			}
		},
		{
			"scope": ["variable.parameter"],
			"settings": {
				"foreground": "#3F4E4F",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"keyword",
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.logical",
				"keyword.declaration",
				"keyword.function",
				"punctuation.accessor",
				"storage.type",
				"storage.modifier",
				"storage.type.function",
				"keyword.declaration.function",
				"storage.type.class",
				"storage.type.struct",
				"storage.type.enum",
				"storage.type.interface",
				"entity.name.namespace",
				"support.other.namespace",
				"support.type.object.module",
				"variable.other.object.package",
				"variable.other.constant.package"
			],
			"settings": {
				"foreground": "#556B2F"
			}
		},
		{
			"scope": [
				"keyword.control",
				"keyword.control.flow",
				"keyword.control.conditional",
				"keyword.control.repeat",
				"keyword.control.import",
				"keyword.control.from",
				"keyword.control.export",
				"keyword.operator"
			],
			"settings": {
				"foreground": "#4C7A2E"
			}
		},
		{
			"scope": ["string", "punctuation.definition.string", "string.template"],
			"settings": {
				"foreground": "#8B4513"
			}
		},
		{
			"scope": [
				"entity.name.function",
				"support.function",
				"variable.function",
				"entity.name.method"
			],
			"settings": {
				"foreground": "#A0522D"
			}
		},
		{
			"scope": ["entity.name.type", "support.type", "support.class"],
			"settings": {
				"foreground": "#B58900"
			}
		},
		{
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#B58900"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"constant.language",
				"constant.character",
				"constant.other",
				"variable.other.constant",
				"number",
				"boolean"
			],
			"settings": {
				"foreground": "#2F4F4F"
			}
		},
		{
			"scope": [
				"punctuation",
				"meta.brace",
				"meta.brace.round",
				"meta.brace.square",
				"meta.brace.curly",
				"punctuation.definition.parameters",
				"punctuation.terminator",
				"punctuation.section",
				"punctuation.separator"
			],
			"settings": {
				"foreground": "#3F4E4F"
			}
		},
		{
			"name": "HTML Tags",
			"scope": [
				"entity.name.tag",
				"punctuation.definition.tag",
				"meta.tag.any"
			],
			"settings": {
				"foreground": "#4C7A2E"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"foreground": "#556B2F"
			}
		},
		{
			"name": "HTML Attribute Values",
			"scope": [
				"string.quoted.double.html",
				"string.quoted.single.html",
				"punctuation.definition.string.begin.html",
				"punctuation.definition.string.end.html"
			],
			"settings": {
				"foreground": "#8B4513"
			}
		},
		{
			"name": "Markdown Links",
			"scope": [
				"markup.link",
				"markup.underline.link",
				"meta.link.inline.markdown"
			],
			"settings": {
				"foreground": "#6A994E"
			}
		},
		{
			"name": "Markdown Code",
			"scope": ["markup.inline.raw", "markup.fenced_code.block"],
			"settings": {
				"foreground": "#B58900"
			}
		},
		{
			"name": "Markdown Italic",
			"scope": ["markup.italic", "punctuation.definition.italic.markdown"],
			"settings": {
				"foreground": "#556B2F",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown Bold",
			"scope": ["markup.bold", "punctuation.definition.bold.markdown"],
			"settings": {
				"foreground": "#6A994E",
				"fontStyle": "bold"
			}
		}
	]
}
