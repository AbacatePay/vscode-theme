{
	"$schema": "vscode://schemas/color-theme",
	"name": "AbacatePay Dark Theme",
	"type": "dark",
	"colors": {
		"activityBar.activeBorder": "#00000000",
		"sideBar.border": "#00000000",
		"editorGroup.border": "#00000000",
		"panel.border": "#00000000",
		"tab.border": "#00000000",
		"tab.activeBorder": "#00000000",
		"tab.activeBorderTop": "#00000000",
		"titleBar.border": "#00000000",
		"statusBar.border": "#00000000",
		"focusBorder": "#00000000",
		"contrastBorder": "#00000000",

		"editor.background": "#101213",
		"editor.foreground": "#ECF0F1",

		"activityBar.background": "#101213",
		"activityBar.foreground": "#C6EF9D",
		"activityBarBadge.background": "#C6EF9D",
		"activityBarBadge.foreground": "#101213",

		"sideBar.background": "#101213",
		"sideBar.foreground": "#ECF0F1",
		"sideBarTitle.foreground": "#C6EF9D",

		"editorGroupHeader.tabsBackground": "#101213",
		"editorGroupHeader.tabsBorder": "#1B1B22",

		"tab.activeBackground": "#161819",
		"tab.activeForeground": "#C6EF9D",
		"tab.inactiveBackground": "#101213",
		"tab.inactiveForeground": "#94A3B8",

		"statusBar.background": "#101213",
		"statusBar.foreground": "#ECF0F1",

		"statusBarItem.hoverBackground": "#1B1B22",
		"statusBarItem.activeBackground": "#1B1B22",

		"statusBar.debuggingBackground": "#101213",
		"statusBar.debuggingForeground": "#ECF0F1",

		"statusBarItem.remoteBackground": "#101213",
		"statusBarItem.remoteForeground": "#ECF0F1",

		"statusBar.noFolderBackground": "#101213",
		"statusBar.noFolderForeground": "#ECF0F1",

		"statusBarItem.prominentBackground": "#101213",
		"statusBarItem.prominentForeground": "#ECF0F1",

		"editor.lineHighlightBackground": "#161819",
		"editor.selectionBackground": "#263238",
		"editorCursor.foreground": "#C6EF9D",

		"editorWhitespace.foreground": "#3D5F61",
		"editorIndentGuide.background1": "#3D5F61",
		"editorIndentGuide.activeBackground1": "#C6EF9D",

		"list.activeSelectionBackground": "#263238",
		"list.activeSelectionForeground": "#C6EF9D",
		"list.hoverBackground": "#263238",
		"list.highlightForeground": "#C6EF9D",

		"titleBar.activeBackground": "#101213",
		"titleBar.activeForeground": "#C6EF9D"
	},
	"tokenColors": [
		{
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.quoted.docstring"
			],
			"settings": {
				"foreground": "#94A3B8",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["variable", "variable.other", "support.variable", "identifier"],
			"settings": {
				"foreground": "#D9F2C7"
			}
		},
		{
			"scope": ["variable.parameter"],
			"settings": {
				"foreground": "#D9F2C7"
			}
		},
		{
			"scope": ["constant.language", "constant.numeric", "number", "boolean"],
			"settings": {
				"foreground": "#7A916E"
			}
		},
		{
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.operator",
				"keyword.declaration",
				"storage.type",
				"storage.modifier",
				"support.type"
			],
			"settings": {
				"foreground": "#7A916E",
				"fontStyle": ""
			}
		},
		{
			"scope": ["string", "punctuation.definition.string", "string.template"],
			"settings": {
				"foreground": "#BFA58A"
			}
		},
		{
			"scope": [
				"entity.name.function",
				"support.function",
				"variable.function",
				"entity.name.method"
			],
			"settings": {
				"foreground": "#D4A373"
			}
		},
		{
			"scope": ["entity.name.type", "support.type", "support.class"],
			"settings": {
				"foreground": "#FAF4C8"
			}
		},
		{
			"scope": [
				"punctuation",
				"meta.brace",
				"punctuation.definition.parameters",
				"punctuation.terminator"
			],
			"settings": {
				"foreground": "#94A3B8"
			}
		},
		{
			"scope": ["markup.heading"],
			"settings": {
				"foreground": "#C6EF9D",
				"fontStyle": "bold"
			}
		},
		{
			"scope": ["markup.link"],
			"settings": {
				"foreground": "#BFA58A"
			}
		},
		{
			"scope": [
				"meta.object-binding-pattern",
				"meta.binding.destructuring",
				"punctuation.section.binding.begin",
				"punctuation.section.binding.end"
			],
			"settings": {
				"foreground": "#BFA58A",
				"fontStyle": ""
			}
		}
	]
}
