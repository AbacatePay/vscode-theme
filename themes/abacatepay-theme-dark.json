{
	"$schema": "vscode://schemas/color-theme",
	"name": "AbacatePay Theme Dark",
	"type": "dark",
	"colors": {
		"editor.background": "#101213",
		"editor.foreground": "#ECF0F1",
		"activityBar.background": "#101213",
		"activityBar.foreground": "#C6EF9D",
		"activityBar.activeBorder": "#C6EF9D",
		"activityBarBadge.background": "#C6EF9D",
		"activityBarBadge.foreground": "#101213",
		"sideBar.background": "#161819",
		"sideBar.foreground": "#ECF0F1",
		"sideBar.border": "#1B1B22",
		"sideBarTitle.foreground": "#C6EF9D",
		"editorGroupHeader.tabsBackground": "#101213",
		"editorGroupHeader.tabsBorder": "#1B1B22",
		"tab.activeBackground": "#161819",
		"tab.activeForeground": "#C6EF9D",
		"tab.activeBorder": "#C6EF9D",
		"tab.inactiveBackground": "#101213",
		"tab.inactiveForeground": "#94A3B8",
		"tab.border": "#1B1B22",
		"statusBar.background": "#101213",
		"statusBar.foreground": "#ECF0F1",
		"statusBar.border": "#1B1B22",
		"statusBarItem.hoverBackground": "#1B1B22",
		"list.activeSelectionBackground": "#263238",
		"list.activeSelectionForeground": "#C6EF9D",
		"list.hoverBackground": "#263238",
		"list.highlightForeground": "#C6EF9D",
		"editor.lineHighlightBackground": "#161819",
		"editor.lineHighlightBorder": "#1B1B22",
		"editor.selectionBackground": "#263238",
		"editorCursor.foreground": "#C6EF9D",
		"editorWhitespace.foreground": "#3D5F61",
		"editorIndentGuide.background1": "#3D5F61",
		"editorIndentGuide.activeBackground1": "#C6EF9D",
		"titleBar.activeBackground": "#101213",
		"titleBar.activeForeground": "#C6EF9D",
		"titleBar.border": "#1B1B22"
	},
	"tokenColors": [
		{
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.quoted.docstring"
			],
			"settings": {
				"foreground": "#94A3B8",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"variable",
				"variable.other",
				"support.variable",
				"identifier",
				"variable.parameter",
				"variable.other.property",
				"variable.other.object.property",
				"meta.object-literal.key",
				"entity.name.variable.field"
			],
			"settings": {
				"foreground": "#D9F2C7"
			}
		},
		{
			"scope": ["variable.parameter"],
			"settings": {
				"foreground": "#D9F2C7",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"variable.other.constant",
				"constant.language",
				"constant.numeric",
				"constant.character"
			],
			"settings": {
				"foreground": "#7A916E"
			}
		},
		{
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.logical",
				"keyword.declaration",
				"keyword.function",
				"punctuation.accessor"
			],
			"settings": {
				"foreground": "#7A916E",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"storage.type",
				"storage.modifier",
				"storage.type.function",
				"keyword.declaration",
				"keyword.declaration.function",
				"storage.type.class",
				"storage.type.struct",
				"storage.type.enum",
				"storage.type.interface",
				"entity.name.namespace",
				"support.other.namespace",
				"support.type.object.module",
				"variable.other.object.package",
				"variable.other.constant.package"
			],
			"settings": {
				"foreground": "#7A916E",
				"fontStyle": "bold"
			}
		},
		{
			"scope": ["string", "punctuation.definition.string", "string.template"],
			"settings": {
				"foreground": "#BFA58A"
			}
		},
		{
			"scope": ["entity.name.function", "support.function", "variable.function"],
			"settings": {
				"foreground": "#D4A373"
			}
		},
		{
			"scope": ["entity.name.type", "support.type", "support.class"],
			"settings": {
				"foreground": "#FAF4C8"
			}
		},
		{
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#FAF4C8"
			}
		},
		{
			"scope": [
				"punctuation",
				"meta.brace",
				"meta.brace.round",
				"meta.brace.square",
				"meta.brace.curly",
				"punctuation.definition.parameters",
				"punctuation.terminator",
				"punctuation.section",
				"punctuation.separator",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#ECF0F1"
			}
		},
		{
			"scope": ["entity.name.tag", "punctuation.definition.tag"],
			"settings": {
				"foreground": "#C6EF9D"
			}
		},
		{
			"scope": ["markup.heading"],
			"settings": {
				"foreground": "#C6EF9D",
				"fontStyle": "bold"
			}
		},
		{
			"scope": ["markup.link"],
			"settings": {
				"foreground": "#BFA58A"
			}
		}
	]
}