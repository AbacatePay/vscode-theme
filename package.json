{
	"name": "abacatepay-theme",
	"displayName": "AbacatePay Theme",
	"description": "A green-first VS Code theme designed for AbacatePay.",
	"version": "0.1.4",
	"publisher": "abacatepay",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Themes"
	],
	"configurationDefaults": {
		"[abacatepay-theme-dark]": {
			"editor.bracketPairColorization.enabled": false
		},
		"[abacatepay-theme-light]": {
			"editor.bracketPairColorization.enabled": false
		}
	},
	"contributes": {
		"themes": [
			{
				"uiTheme": "vs-dark",
				"label": "AbacatePay Dark Theme",
				"path": "./themes/abacatepay-theme-dark.json"
			},
			{
				"uiTheme": "vs",
				"label": "AbacatePay Light Theme",
				"path": "./themes/abacatepay-theme-light.json"
			}
		]
	},
	"module": "index.ts",
	"type": "module",
	"private": true,
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@types/bun": "latest"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"scripts": {
		"biome:check": "bunx biome check --write ."
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/almeidazs/abacatepay-theme"
	},
	"license": "MIT",
	"icon": "assets/logo.png",
	"homepage": "https://github.com/almeidazs/abacatepay-theme",
	"keywords": [
		"theme",
		"dark theme",
		"light theme",
		"green",
		"abacatepay"
	]
}
